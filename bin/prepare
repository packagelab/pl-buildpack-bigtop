  - libsqlite3-dev
  - libssl-dev
  - libxml2-dev
  - libxslt1-dev
  - libzip-dev
  - maven
  - openjdk-7-jdk
  - pkg-config
  - python-dev
  - python-setuptools
  - sharutils
  - unzip
  - xmlto
